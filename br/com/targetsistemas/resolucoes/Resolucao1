
public class Target {

    public static void main(String[] args) {
        
        Integer indice = 13;
        Integer soma = 0;
        Integer k = 0;

        while (k < indice) {
            k += 1;
            soma += k;
        }

        System.out.println("Ao final do processamento, o valor da variável SOMA é: " + soma);

    }

}
